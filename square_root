// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int i,j,k,n = 6,count = 1;
    int arr[n];
    printf("Enter the array elements \n");
    for(j=0;j<n;j++){
        printf("Enter %d arr element : ",j+1);
        scanf("%d",&arr[j]);
    }
    printf("Array elements : \n");
    for(k=0;k<n;k++){
        printf("%d\t",arr[k]);
    }
    int e;
    printf("\nEnter the element : ");
    scanf("%d",&e);
    
    int l=0;
    int h=n;
    int mid;
    while(l<h){
        mid = (l+h)/2;
        if((arr[mid]*arr[mid]) == e){
            printf("The square root of %d is: %d\n found at location %d",e,arr[mid],mid);
            break;
        }
        else if((arr[mid]*arr[mid])<e){
            l = mid;
        }
        else{
            h = mid;
        }
    }

    return 0;
}
